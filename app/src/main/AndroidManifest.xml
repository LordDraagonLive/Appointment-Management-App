<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bacdevelopers.appointmentmanagementapplication">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".mainpagecont.MainPageActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".createpagecont.CreatePage" />
        <activity android:name=".modifycont.DeleteMenuActivity" />
        <activity android:name=".modifycont.ModifyActivity" />
        <activity android:name=".modifycont.UpdateActivity" />
        <activity android:name=".modifycont.MoveActivity"/>
    </application>

</manifest>